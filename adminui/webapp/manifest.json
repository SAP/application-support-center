{
	"sap.app": {
		"id": "asc.admin",
		"applicationVersion": {
			"version": "1.0.0"
		}
	},
	"sap.ui5": {
        "rootView": {
			"viewName": "asc.admin.view.App",
			"type": "XML",
			"async": true,
			"id": "fcl"
		},
		"resources": {
			"css": [
				{
					"uri": "css/style.css"
				}
			]
		},
		"dependencies": {
			"libs": {
				"sap.ui.core": {
				},
				"sap.m": {
				},
				"sap.f": {
				},
				"sap.ui.layout": {
				},
				"sap.uxap": {
				}
			}
		},
		"config": {
			"fullWidth": true,
			"reportingId": "ASCv2"
		},
		"routing": {
			"config": {
				"routerClass": "sap.f.routing.Router",
				"viewType": "XML",
				"viewPath": "asc.admin.view",
				"controlId": "fcl",
				"transition": "slide",
				"bypassed": {
				},
				"async": true
			},
			"routes": [
				{
					"pattern": "",
					"name": "dashboard",
					"target": [
						"dashboard"
					],
					"layout": "OneColumn"
				},{
					"pattern": "reporting",
					"name": "reporting",
					"target": [
						"reporting"
					],
					"layout": "OneColumn"
				},{
					"pattern": "admin",
					"name": "admin",
					"target": [
						"admin"
					],
					"layout": "OneColumn"
				},{
					"pattern": "apps",
					"name": "applist",
					"target": [
						"applist"
					],
					"layout": "OneColumn"
				},
				{
					"pattern": "apps/{app}",
					"name": "viewapp",
					"target": [
						"applist",
						"viewapp"
					],
					"layout": "TwoColumnsMidExpanded"
				},
				{
					"pattern": "addapp",
					"name": "addapp",
					"target": [
						"applist",
						"addapp"
					],
					"layout": "TwoColumnsMidExpanded"
				},
				{
					"pattern": "apps/{app}/edit",
					"name": "editapp",
					"target": [
						"applist",
						"editapp"
					],
					"layout": "TwoColumnsMidExpanded"
				},

				{
					"pattern": "admin/addsetting",
					"name": "addadminsetting",
					"target": [
						"admin",
						"addadminsetting"
					],
					"layout": "TwoColumnsBeginExpanded"
				},{
					"pattern": "admin/editsetting/{setting_id}",
					"name": "editadminsetting",
					"target": [
						"admin",
						"editadminsetting"
					],
					"layout": "TwoColumnsBeginExpanded"
				},

				{
					"pattern": "admin/addnotification",
					"name": "addadminnotification",
					"target": [
						"admin",
						"addadminnotification"
					],
					"layout": "TwoColumnsBeginExpanded"
				},
				{
					"pattern": "admin/editnotification/{notification_id}",
					"name": "editadminnotification",
					"target": [
						"admin",
						"editadminnotification"
					],
					"layout": "TwoColumnsBeginExpanded"
				},
				




				{
					"pattern": "addcontact",
					"name": "addcontact",
					"target": [
						"viewcontacts",
						"addcontact"
					],
					"layout": "ThreeColumnsMidExpanded"
				},
				{
					"pattern": "contacts",
					"name": "viewcontacts",
					"target": [
						"applist",
						"viewcontacts"
					],
					"layout": "TwoColumnsMidExpanded"
				},
				{
					"pattern": "contacts/{contact}",
					"name": "viewcontact",
					"target": [
						"applist",
						"viewcontacts",
						"viewcontact"
					],
					"layout": "ThreeColumnsMidExpanded"
				},
